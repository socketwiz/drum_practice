{
  "name": "drum-practice",
  "version": "0.0.1",
  "description": "App to manage songs for a drum practice",
  "scripts": {
    "dev": "webpack --stats-error-details true --config webpack-dev.config.js",
    "build": "webpack --config webpack-prod.config.js",
    "lint": "eslint src",
    "postupdates": "npm install",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch",
    "updates": "ncu -u"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:socketwiz/drum_practice.git"
  },
  "author": "Ricky Nelson <rickyn@socketwiz.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "@headlessui/react": "^1.4.0",
    "@heroicons/react": "^1.0.3",
    "js-cookie": "^3.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-i18next": "^11.11.4",
    "react-router-dom": "^5.2.0",
    "rxjs": "7.3.0",
    "zol-class-names": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-check-updates": "^11.8.3",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "private": true
