    "electron": "4.0.4",
    "electron-packager": "13.0.1",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.8.0",
    "mocha": "5.2.0",
    "npm-check-updates": "2.15.0",
    "react-test-renderer": "16.7.0",
    "redux-devtools-extension": "2.13.8",
